# TrailVerse Client Environment Variables
# Copy this file to .env and fill in your actual values

# App Configuration
REACT_APP_NAME=TrailVerse
REACT_APP_DOMAIN=www.nationalparksexplorerusa.com
VITE_API_URL=http://localhost:5001/api
REACT_APP_CONTACT_EMAIL=trailverseteam@gmail.com
REACT_APP_SUPPORT_EMAIL=trailverseteam@gmail.com
REACT_APP_WEBSITE_URL=https://www.nationalparksexplorerusa.com

# API Configuration
REACT_APP_NPS_API_KEY=your-nps-api-key
REACT_APP_OPENWEATHER_API_KEY=your-openweather-api-key

# Supabase Configuration (if using)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_APP_ENV=development

# Analytics (optional)
REACT_APP_GA_TRACKING_ID=your-google-analytics-id

# Admin Access (SECURITY: Never commit real credentials to git)
REACT_APP_ADMIN_EMAIL=admin@example.com
REACT_APP_ADMIN_PASSWORD=your-secure-admin-password

# Environment
REACT_APP_ENV=development
