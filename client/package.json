{
  "name": "trailverse-client",
  "version": "1.0.0",
  "description": "TrailVerse - AI-Powered National Parks Trip Planner",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-leaflet": "^4.2.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "react-scripts": "5.0.1",
    "remark-gfm": "^4.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true DISABLE_ESLINT_PLUGIN=true TSC_COMPILE_ON_ERROR=true CI=true react-scripts build",
    "test": "craco test",
    "test:vitest": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:vitest:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:vitest && npm run test:e2e",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@craco/craco": "^7.1.0",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "jsdom": "^25.0.1",
    "msw": "^2.6.8",
    "playwright": "^1.49.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^4.9.5",
    "vitest": "^2.1.8"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "schema-utils": "3.3.0",
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2",
    "terser-webpack-plugin": "5.3.10",
    "css-minimizer-webpack-plugin": "3.4.1"
  }
}
